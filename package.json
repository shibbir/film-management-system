{
  "name": "film-manager",
  "version": "1.0.0",
  "scripts": {
    "start": "run-p webpack:development pm2:development",
    "webpack:development": "cross-env NODE_ENV=development webpack --progress --watch --stats normal",
    "pm2:development": "cross-env NODE_ENV=development pm2 start src/config/server.js --watch --no-daemon --name film-manager",
    "webpack:production": "cross-env NODE_ENV=production webpack --progress",
    "pm2:production": "cross-env NODE_ENV=production pm2 start src/config/server.js --name film-manager",
    "production": "run-s webpack:production pm2:production",
    "seed": "node seeder.js",
    "stop": "pm2 delete film-manager",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Shibbir Ahmed",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.25.0",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "express-hbs": "^2.4.0",
    "fomantic-ui-css": "^2.8.8",
    "formik": "^2.2.9",
    "glob": "^7.2.0",
    "izitoast": "^1.4.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.24.4",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-star-ratings": "^2.3.0",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "sequelize": "^6.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "pm2": "^5.1.2",
    "terser-webpack-plugin": "^5.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3"
  }
}
